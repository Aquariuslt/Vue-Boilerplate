{
  "name": "vue-boilperlate",
  "version": "1.0.1",
  "description": "A Vue boilerplate like Angular build flow.",
  "author": "Aquariuslt<superaquariuslt@gmail.com>",
  "repository": "https://github.com/aquariuslt/vue-boilerplate",
  "license": "MIT",
  "scripts": {
    "clean": "gulp clean",
    "dev": "gulp serve",
    "build": "gulp build",
    "lint": "gulp lint",
    "test": "gulp test"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "vue": "^2.5.17",
    "vue-i18n": "^7.8.0",
    "vue-material": "^1.0.0-beta-10.2",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@babel/core": "7.0.0",
    "@babel/plugin-external-helpers": "7.0.0",
    "@babel/plugin-proposal-class-properties": "7.0.0",
    "@babel/plugin-transform-runtime": "7.0.0",
    "@babel/polyfill": "7.0.0",
    "@babel/preset-env": "7.0.0",
    "@babel/register": "7.0.0",
    "@babel/runtime": "7.0.0",
    "@types/extract-text-webpack-plugin": "^3.0.2",
    "@types/jest": "^23.1.1",
    "@types/marked": "^0.3.0",
    "@types/mocha": "^5.2.0",
    "@types/moxios": "^0.4.8",
    "@types/sinon": "^5.0.1",
    "@types/webpack": "^4.4.11",
    "@vue/cli-plugin-unit-jest": "^3.0.1",
    "@vue/test-utils": "1.0.0-beta.24",
    "ajv": "^6.2.0",
    "autoprefixer": "^9.1.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.4.2",
    "babel-loader": "^8.0.0",
    "babel-plugin-lodash": "^3.3.4",
    "connect-history-api-fallback": "^1.5.0",
    "copy-webpack-plugin": "^4.5.1",
    "coveralls": "^3.0.0",
    "css-loader": "^0.28.11",
    "eslint": "^5.4.0",
    "eslint-plugin-vue": "^5.0.0-beta.3",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "fancy-log": "^1.3.2",
    "file-loader": "^1.1.11",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "glob": "^7.1.2",
    "gulp": "^3.9.1",
    "gulp-inject-string": "^1.1.1",
    "gulp-rename": "^1.2.2",
    "gulp-rimraf": "^0.2.2",
    "gulp-sequence": "^1.0.0",
    "html-webpack-plugin": "^3.2.0",
    "http-proxy-middleware": "^0.18.0",
    "jest": "^23.1.0",
    "jest-cli": "^23.1.0",
    "jest-serializer-vue": "^2.0.2",
    "js-yaml": "^3.11.0",
    "less": "^3.0.2",
    "less-loader": "^4.1.0",
    "lodash": "^4.17.10",
    "lodash-webpack-plugin": "^0.11.5",
    "mkdirp": "^0.5.1",
    "moxios": "^0.4.0",
    "normalize.css": "^8.0.0",
    "offline-plugin": "^5.0.0",
    "optimize-css-assets-webpack-plugin": "^4.0.0",
    "optimize-js-plugin": "^0.0.4",
    "ora": "^2.0.0",
    "sinon": "^6.1.5",
    "style-loader": "^0.23.0",
    "url-loader": "^1.1.1",
    "vue-jest": "^2.6.0",
    "vue-loader": "^15.4.1",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^4.17.1",
    "webpack-dev-middleware": "^3.2.0",
    "webpack-dev-server": "^3.1.6",
    "webpack-hot-middleware": "^2.22.1",
    "webpack-merge": "^4.1.4",
    "webpack-pwa-manifest": "^3.7.1"
  },
  "jest": {
    "verbose": true,
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-vue"
    ],
    "testMatch": [
      "<rootDir>/test/**/*.spec.js"
    ],
    "setupFiles": [
      "<rootDir>/test/unit/shared/hooks/setup"
    ],
    "coverageDirectory": "<rootDir>/test/unit/coverage",
    "collectCoverageFrom": [
      "src/**/*.{js}",
      "src/**/*.{vue}",
      "!**/node_modules/**",
      "!**/config/**",
      "!**/tasks/**",
      "!src/*.js",
      "!**/*.{api.js}",
      "!**/core/index.js"
    ]
  }
}
